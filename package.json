{
    "name": "json3d",
    "version": "0.0.1",
    "description": "A web app to view json files in 3d using Babylon.js",
    "main": "index.ts",
    "dependencies": {
        "@babylonjs/core": "preview",
        "@babylonjs/loaders": "preview",
        "@babylonjs/materials": "preview",
        "gh-pages": "^3.1.0",
        "@types/node": "^14.14.20",
        "node-fetch": "^2.6.1"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "clean-webpack-plugin": "^3.0.0",
        "eslint": "^6.8.0",
        "file-loader": "^6.0.0",
        "html-webpack-plugin": "^4.2.0",
        "source-map-loader": "^0.2.4",
        "ts-loader": "^6.2.1",
        "typescript": "^3.8.3",
        "url-loader": "^4.1.0",
        "webpack": "^4.43.0",
        "webpack-bundle-analyzer": "^4.3.0",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.3",
        "webpack-merge": "^4.2.2"
    },
    "scripts": {
        "start": "npx webpack-dev-server --config webpack.dev.js",
        "build": "npx webpack --config webpack.prod.js",
        "lint": "npx eslint . --ext .ts,.tsx",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d dist"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/christiansimms/json3d.git"
    },
    "keywords": [
        "Babylon.js",
        "webpack",
        "typescript",
        "es6",
        "json"
    ],
    "author": {
        "name": "Christian Simms",
        "email": "christian.simms+json3d@gmail.com",
        "url": "http://csimms.botonomy.com/"
    },
